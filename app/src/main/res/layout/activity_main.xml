                            android:layout_height="64dp"
                            android:src="@drawable/ic_account_balance_black_24dp"
                            android:background="@drawable/drawableblack"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:textColor="@color/fbutton_color_nephritis"
                            android:textStyle="bold"
                            android:textSize="20dp"
                            android:textAlignment="center"
                            android:paddingTop="20dp"
                            android:textAllCaps="true"
                            android:layout_height="wrap_content"
                            android:text="Veterinary"/>
                        <View
                            android:layout_width="wrap_content"
                            android:background="@color/fbutton_color_silver"
                            android:layout_height="1dp"
                            />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="230dp">
                <androidx.cardview.widget.CardView
                    android:layout_width="160dp"
                    android:layout_margin="10dp"
                    android:id="@+id/money"
                    android:layout_height="190dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="64dp"
                            android:layout_height="64dp"
                            android:src="@drawable/ic_build_black_24dp"
                            android:background="@drawable/drawableblue"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:textColor="@color/fbutton_color_nephritis"
                            android:textStyle="bold"
                            android:textSize="20dp"
                            android:paddingTop="20dp"
                            android:textAllCaps="true"
                            android:textAlignment="center"
                            android:layout_height="wrap_content"
                            android:text="Money"/>
                        <View
                            android:layout_width="wrap_content"
                            android:background="@color/fbutton_color_silver"
                            android:layout_height="1dp"
                            />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="160dp"
                    android:layout_margin="10dp"
                    android:id="@+id/hair"
                    android:layout_height="190dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="64dp"
                            android:layout_height="64dp"
                            android:src="@drawable/ic_account_balance_black_24dp"
                            android:background="@drawable/drawableblack"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:textColor="@color/fbutton_color_nephritis"
                            android:textStyle="bold"
                            android:textSize="20dp"
                            android:textAlignment="center"
                            android:paddingTop="20dp"
                            android:textAllCaps="true"
                            android:layout_height="wrap_content"
                            android:text="Hair Dressing"/>
                        <View
                            android:layout_width="wrap_content"
                            android:background="@color/fbutton_color_silver"
                            android:layout_height="1dp"
                            />
