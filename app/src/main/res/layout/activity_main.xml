                            android:background="@color/fbutton_color_silver"
                            android:layout_height="1dp"
                            />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="160dp"
                    android:layout_margin="10dp"
                    android:id="@+id/Veterinary"
